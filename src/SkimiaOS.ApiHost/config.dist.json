{
  "Host": {
    "Urls": [ "http://::80", "http://0.0.0.0:80" ]
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Extensions": {
    // Please keep in mind that you have to change '\' to '/' on Linux-based systems
    "Path": "\\Plugins"
  },
  "Data": {
    "DefaultConnection": {
      // Please keep in mind that you have to change '\' to '/' on Linux-based systems
      "ConnectionString": "Data Source=..\\..\\..\\db.sqlite"
    }
  },
  "Plugins": {
    "StaticFiles": {
      "Path": "apps",
      "AutomaticResolution": false,
      "Applications": [
        {
          "Name": "os.web",
          "Path": "apps\\os.web",
          "BindTo": "/",
          "Rewrite": {
            "Apache": "rewrite.apache"
          }
        }
      ]
    },
    //Dont keep this config in production is not secure
    "Identity":{
      "Password":{
        "RequireDigit": false,
        "RequireNonAlphanumeric": false,
        "RequireUppercase":false,
        "RequireLowercase":false,
        "RequiredLength": 6
      }
    }
  
  
  }
}